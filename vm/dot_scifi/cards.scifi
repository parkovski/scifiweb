Card collectable group:
  has amount;
  property foo array of integer;
  property bar array x 10 of array of CommonCard;
  has collectable group [CommonCard, RareCard];
  has upgrades;
end;

CommonCard collectable group:
  has collectable [
    Bomb, Bow, Sword
  ];
end;

RareCard collectable group:
  has collectable [
    FireBow, FreezeRay
  ];
end;

# Common cards
Bomb collectable;
Bow collectable;
Sword collectable;

# Rare cards
FireBow collectable;
FreezeRay collectable;