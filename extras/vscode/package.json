{
  "name": "scifiweb-vscode-grammar",
  "version": "0.1.0",
  "publisher": "parkovski",
  "engines": {
    "vscode": "^1.15.0"
  },
  "description": "SciFiWeb game definition langauge support",
  "author": "parkovski",
  "license": "ISC",
  "displayName": "SciFiWeb Language",
  "categories": [
    "Languages"
  ],
  "contributes": {
    "languages": [
      {
        "extensions": [ ".scifi" ],
        "aliases": [ "SciFiWeb" ],
        "id": "scifiweb",
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "path": "./syntaxes/scifiweb.tmLanguage.json",
        "language": "scifiweb",
        "scopeName": "scifiweb"
      }
    ]
  }
}
